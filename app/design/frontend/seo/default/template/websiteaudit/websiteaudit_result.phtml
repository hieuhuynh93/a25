<script>
    function showData(action, txtBox, rowLoader, result) {
        var url = '<?php echo Mage::getBaseUrl(); ?>websiteaudit/index/' + action + '?domain_id=<?php echo $domainId; ?>';
        var whois = document.getElementById(txtBox).value;
        if (parseInt(whois) <= 0) {
            jQuery("#" + rowLoader).show();
            jQuery.ajax({
                type: "POST",
                url: url,
                data: '',
                success: function (data)
                {
                    document.getElementById(result).innerHTML = data;
                    document.getElementById(txtBox).value = 1;
                    if(txtBox == 'SocialMediaAnalysis'){
                        jQuery('#datatable-top-hastags').dataTable({
                            "scrollY":        "600px",
                            "scrollCollapse": true,
                            "paging":         false
                        });
                        jQuery('#datatable-related-keywords').dataTable({
                            "scrollY":        "600px",
                            "scrollCollapse": true,
                            "paging":         false
                        });
                    }
                    if(txtBox == 'KeywordOccourance'){
                        createCustomDataTable('datatable-one-phrase');
                        createCustomDataTable('datatable-two-phrase');
                        createCustomDataTable('datatable-three-phrase');
                        createCustomDataTable('datatable-four-phrase');
                    }
                    
                    if(action == 'getWhoIs'){
                        //For Merging requests Whois and IP Information
                        //Need to remove loader from old showdata function
                        showData('getIpDetails', 'IpInformation', 'rowLoaderWhoisInformation', 'resultIpInformation');
                    }else if(action == 'getMozDetails'){
                        //For Merging requests MOZ Information and Build with Technology
                        //Need to remove loader from old showdata function
                        showData('getBuildWith', 'buildWithTechnology', 'rowLoaderMozInformation', 'resultbuildWithTechnology');
                    } else {
                        jQuery("#" + rowLoader).hide();    
                    }
                }
            });
        }
    }
    
    function createCustomDataTable(dataTableId){
        jQuery('#'+dataTableId).dataTable({
                            "scrollY":        "600px",
                            "scrollCollapse": true,
                            "paging":         false
                        });
    }
</script>