<script>
    function deleteKeywords(keyword_ids) {
        var linkDetailsId = "<?php echo $linkDetailsId; ?>";
        var ids = [];
        jQuery.each($("input[name='" + keyword_ids + "']:checked"), function () {
            ids.push(jQuery(this).val());
        });
        var keywordIds = ids.join(",");
        var url = "<?php echo Mage::getBaseUrl(); ?>linkanalysis/index/deletekeyword?keywords_name=" + keywordIds + "&linkDetailsId=" + linkDetailsId;

        if (keywordIds.length > 0) {
            jQuery("#pre-loader").css("opacity", '0.8');
            jQuery("#pre-loader").show();
            jQuery.ajax({
                type: "POST",
                url: url,
                data: "",
                success: function (data)
                {
                    var objData = jQuery.parseJSON(data);
                    document.getElementById("keywordTableCustom").innerHTML = objData.content;
                    jQuery("#pre-loader").hide();
                }
            });
        } else {
            alert("Please select keywords");
        }
    }

    function addKeywords(linkDetailsId) {
        var keywords_name = jQuery("#keywords_name").val();
        var url = "<?php echo Mage::getBaseUrl(); ?>linkanalysis/index/addkeyword?keywords_name=" + keywords_name + "&linkDetailsId=" + linkDetailsId;
        if (keywords_name == '' || keywords_name == null) {
            document.getElementById("submitkeywordname").click();
        } else {
            jQuery("#pre-loader").css("opacity", '0.8');
            jQuery("#pre-loader").show();
            jQuery.ajax({
                type: "POST",
                url: url,
                data: "",
                success: function (data)
                {
                    var objData = jQuery.parseJSON(data);
                    document.getElementById("keywordTableCustom").innerHTML = objData.content;
                    jQuery("#pre-loader").hide();
                }
            });
        }
    }

    function showMetaTitle(metaTitle) {
        jQuery("#addKeyWordSec").hide();
        jQuery("#metatitlesection").show();
        jQuery("#section_title").html("<p style='padding-left:10px;' class='card-text text-dark'><b>META TITLE</b></p>");
        if (metaTitle.length > 1) {
            jQuery("#metatitlesection").html(metaTitle);
        } else {
            jQuery("#metatitlesection").html('<label style="font-size:13px;padding-bottom: 10px; padding-top: 8px;" class="badge badge-danger">Meta Title Not Found!</label>');
        }
    }

    function showMetaDescription(metaDescription) {
        jQuery("#addKeyWordSec").hide();
        jQuery("#metatitlesection").show();
        jQuery("#section_title").html("<p style='padding-left:10px;' class='card-text text-dark'><b>META DESCRIPTION</b></p>");
        if (metaDescription.length > 1) {
            jQuery("#metatitlesection").html(metaDescription);
        } else {
            jQuery("#metatitlesection").html('<label style="font-size:13px;padding-bottom: 10px;padding-top: 8px;" class="badge badge-danger">Meta Description Not Found!</label>');
