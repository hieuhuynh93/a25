<script>
    function showScroll(id) {
        setTimeout(function () {
            document.getElementById(id).setAttribute("style", "width: 100%; height: 220px !important; overflow: auto;");
        }, 2000);
    }

    function showKeywordSearchUrls(id, keywords_name) {
        var textBoxVal = document.getElementById("custom_keyword" + id);
        if (textBoxVal.value == '' || textBoxVal.value == null) {
            var url = "<?php echo Mage::getBaseUrl(); ?>linkanalysis/index/keywordsearchurl?keywords_name=" + keywords_name;
            jQuery("#rowLoader").css("opacity", '0.8');
            //jQuery("#pre-loader").show();
            jQuery.ajax({
                type: "POST",
                url: url,
                data: "",
                success: function (data)
                {
                    //jQuery("#pre-loader").hide();
                    //jQuery("#collapse-backlinks" + id).val(data);
                    document.getElementById("collapse-backlinks"+id).innerHTML = data;
                    textBoxVal.value = keywords_name;
                    setTimeout(function () {
                        document.getElementById("collapse-backlinks" + id).setAttribute("style", "width: 100%; height: 220px !important; overflow: auto;");
                    }, 2000);
                }
            });
        } else {
            setTimeout(function () {
                document.getElementById("collapse-backlinks" + id).setAttribute("style", "width: 100%; height: 220px !important; overflow: auto;");
            }, 2000);
        }
    }

</script>