                            <?php $count = 0; ?>
                            <?php foreach (explode(",", $keywords) as $keywordCustomForGraph): ?>
                                <?php
                                    $graphModel = Mage::getModel('graph/graph')
                                                    ->getCollection()
                                                    ->addFieldToFilter('linkdetails_id', '3')
                                                    ->addFieldToFilter('element_name', $keywordCustomForGraph)
                                                    ->addFieldToFilter('element_type', '1')
                                                    ->addFieldToFilter('status', '1');
                                    $arrayGoogleRank = array();
                                    $arrayYahooRank = array();
                                    $arrayBingRank = array();
                                    $arrayYandexRank = array();
                                    $arrayDay = array();
                                    foreach ($graphModel as $graph) {
                                        array_push($arrayGoogleRank, $graph->getGoogleRank());
                                        array_push($arrayYahooRank, $graph->getYahooRank());
                                        array_push($arrayBingRank, $graph->getBingRank());
                                        array_push($arrayYandexRank, $graph->getYandexRank());
                                        array_push($arrayDay, '"'.$graph->getDay() . '/' . $graph->getMonth() . '/' . $graph->getYear() . '"');
                                        //$day = $graph->getDay();
                                        //$month = $graph->getMonth();
                                        //$year = $graph->getYear();
                                    }
                                ?>
                                <div style="width: 100%; text-align: center; padding-bottom: 10px;">
                                    <center>
                                        <table style="width: 50%;">
                                            <tr>
                                                <td style="background-color: #007bff; width: 25%; color: #fff; font-size: 10px; font-weight: 600; text-align: center;">GOOGLE</td>
                                                <td style="background-color: #28a745; width: 25%; color: #fff; font-size: 10px; font-weight: 600; text-align: center;">YAHOO</td>
                                                <td style="background-color: #444444; width: 25%; color: #fff; font-size: 10px; font-weight: 600; text-align: center;">BING</td>
                                                <td style="background-color: #dc3545; width: 25%; color: #fff; font-size: 10px; font-weight: 600; text-align: center;">YANDEX</td>
                                            </tr>
                                        </table>
                                    </center>
                                </div>
                                <h6 style="padding-left: 15px;">Keyword: <span><?php echo $keywordCustomForGraph; ?></span></h6>
                                <hr>
                                <canvas id="chBar_<?php echo $keywordCustomForGraph; ?>" height="100"></canvas>
                                <script>
                                    var colors = ['#007bff', '#28a745', '#444444', '#c3e6cb', '#dc3545'];

                                    var chBar = document.getElementById("chBar_<?php echo $keywordCustomForGraph; ?>");
                                    var chartData = {
                                        labels: [<?php echo implode(",", $arrayDay); ?>],
                                        datasets: [{
                                                data: [<?php echo implode(",", $arrayGoogleRank); ?>],
                                                backgroundColor: colors[0]
                                            },
                                            {
                                                data: [<?php echo implode(",", $arrayYahooRank); ?>],
                                                backgroundColor: colors[1]
                                            },
                                            {
                                                data: [<?php echo implode(",", $arrayBingRank); ?>],
                                                backgroundColor: colors[2]
                                            },
                                            {
                                                data: [<?php echo implode(",", $arrayYandexRank); ?>],
                                                backgroundColor: colors[4]
                                            }]
                                    };

                                    if (chBar) {
                                        new Chart(chBar, {
                                            type: 'bar',
                                            data: chartData,
                                            options: {
                                                scales: {
                                                    xAxes: [{
                                                            barPercentage: 0.4,
                                                            categoryPercentage: 0.5
                                                        }],
                                                    yAxes: [{
                                                            ticks: {
                                                                beginAtZero: false
                                                            }
                                                        }]
                                                },
                                                legend: {
                                                    display: false
                                                }
                                            }
                                        });
                                    }
                                </script>
                                <?php $count++; ?>
                            <?php endforeach; ?>
