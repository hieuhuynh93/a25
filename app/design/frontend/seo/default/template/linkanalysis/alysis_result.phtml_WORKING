<script>
    var codeTrendsKey = '';
    var codeTrendsCountry = '';
    var codeTrendsTime = '';

    function getKeywordDensityAndPossibilitySpam() {
        jQuery("#rowLoaderKDPS").show();
        jQuery("#kdpsBoby").hide();
        var url = "<?php echo Mage::getBaseUrl(); ?>linkanalysis/index/KeywordDensityAndPossibilitySpam?linkdetails_id=<?php echo $linkDetailsId; ?>";
                jQuery.ajax({
                    type: "POST",
                    url: url,
                    data: "",
                    success: function (data)
                    {
                        var objData = jQuery.parseJSON(data);
                        document.getElementById("bodyOfSinglePhrase").innerHTML = objData.one_phrase;
                        document.getElementById("bodyOfTwoPhrase").innerHTML = objData.two_phrase;
                        document.getElementById("bodyOfThreePhrase").innerHTML = objData.three_phrase;
                        document.getElementById("bodyOfFourPhrase").innerHTML = objData.four_phrase;

                        jQuery("#kdpsBoby").show();
                        jQuery("#rowLoaderKDPS").hide();

                    }
                });
            }

            function setTrendsTime(e) {
                codeTrendsTime = e.value;
                var country = document.getElementById("country").value;
                codeTrendsCountry = country;
                var newURL = "<?php echo Mage::getBaseUrl() . 'keyword-trends?google_trends_keywords='; ?>" + codeTrendsKey + "&google_trends_geo=" + codeTrendsCountry + "&google_trends_time=" + codeTrendsTime;
                var iframe = document.getElementById('trendsPopupFrame');
                iframe.src = newURL;
            }

            function setTrendsCountry(e) {
                codeTrendsTime = document.getElementById("google_trends_time").value;
                codeTrendsCountry = e.value;
                var newURL = "<?php echo Mage::getBaseUrl() . 'keyword-trends?google_trends_keywords='; ?>" + codeTrendsKey + "&google_trends_geo=" + codeTrendsCountry + "&google_trends_time=" + codeTrendsTime;
                var iframe = document.getElementById('trendsPopupFrame');
                iframe.src = newURL;
            }

            function showGoogleTrends(key) {
                codeTrendsTime = document.getElementById("google_trends_time").value;
                var country = document.getElementById("country").value;
                codeTrendsCountry = country;
                document.getElementById("trendsPopupFrame").setAttribute("src", "");
                codeTrendsKey = key;
                var url = "<?php echo Mage::getBaseUrl() . 'keyword-trends?google_trends_keywords='; ?>" + codeTrendsKey + "&google_trends_geo=" + codeTrendsCountry + "&google_trends_time=" + codeTrendsTime;
                document.getElementById("trendsPopupFrame").setAttribute("src", url);
                document.getElementById("btnGoogleTrends").click();
            }

            function compareKeywords() {
                var ids = [];
                jQuery.each($("input[name='keywords']:checked"), function () {
                    ids.push(jQuery(this).val());
                });

                var key = ids.join(",");
                codeTrendsKey = key;
                codeTrendsCountry = document.getElementById("country").value;
                codeTrendsTime = document.getElementById("google_trends_time").value;
                document.getElementById("trendsPopupFrame").setAttribute("src", "");
                var url = "<?php echo Mage::getBaseUrl() . 'keyword-trends?google_trends_keywords='; ?>" + codeTrendsKey + "&google_trends_geo=" + codeTrendsCountry + "&google_trends_time=" + codeTrendsTime;
                document.getElementById("trendsPopupFrame").setAttribute("src", url);
                document.getElementById("btnGoogleTrends").click();
            }

            function getKeywordSuggession() {
                jQuery("#rowLoaderKeywordSuggession").show();
                jQuery("#keywordSuggessionBoby").hide();
                var url = "<?php echo Mage::getBaseUrl(); ?>linkanalysis/index/getkeywordsuggession?linkdetails_id=<?php echo $linkDetailsId; ?>";
                        jQuery.ajax({
                            type: "POST",
                            url: url,
                            data: "",
                            success: function (data)
                            {
                                jQuery("#keywordSuggessionData").html(data);
                                jQuery("#keywordSuggessionBoby").show();
                                jQuery("#rowLoaderKeywordSuggession").hide();

                            }
                        });
                    }

                    function showKeywordAnalysisResult(keyword) {
                        jQuery("#pre-loader").css("opacity", '0.8');
                        jQuery("#pre-loader").show();

                        jQuery("#keyword_analysis_result_modal_body").hide();
                        var url = "<?php echo Mage::getBaseUrl(); ?>linkanalysis/index/getkeywordanalysisresulthtml?linkdetails_id=<?php echo $linkDetailsId; ?>&keyword=" + keyword;
                        jQuery("#keyword_title").html("Analysis Result: " + keyword);
                        jQuery.ajax({
                            type: "POST",
                            url: url,
                            data: "",
                            success: function (data)
                            {
                                jQuery("#pre-loader").hide();
                                jQuery("#keyword_analysis_result_modal_body").show();
                                jQuery("#keyword_analysis_result_modal_body").html(data);
                                //document.getElementById("keyword_analysis_status_box").value = 1;
                                document.getElementById("linkanalysis_result_modal").click();
                            }
                        });
                    }

                    function getKeywordAnalysis() {
                        var keyword_analysis_status_box = document.getElementById("keyword_analysis_status_box").value;
                        if (keyword_analysis_status_box == '0') {
                            jQuery("#rowLoaderKeywordAnalysis").show();
                            jQuery("#collapse-occupiers6").hide();
                            var url = "<?php echo Mage::getBaseUrl(); ?>linkanalysis/index/getkeywordanalysis?linkdetails_id=<?php echo $linkDetailsId; ?>";
                                        jQuery.ajax({
                                            type: "POST",
                                            url: url,
                                            data: "",
                                            success: function (data)
                                            {
                                                //var objData = jQuery.parseJSON(data);
                                                jQuery("#collapse-occupiers6").show();
                                                jQuery("#rowLoaderKeywordAnalysis").hide();
                                                jQuery("#link_analysis_result_custom").html(data);
                                                document.getElementById("keyword_analysis_status_box").value = 1;
                                            }
                                        });
                                    }
                                }

                                function getLinkPosition() {
                                    var link_poition_status_box = document.getElementById("link_poition_status_box").value;
                                    if (link_poition_status_box == '0') {
                                        jQuery("#rowLoaderLinkPosition").show();
                                        jQuery("#collapse-occupiers7").hide();
                                        var url = "<?php echo Mage::getBaseUrl(); ?>linkanalysis/index/getlinkposition?linkdetails_id=<?php echo $linkDetailsId; ?>";
                                                    jQuery.ajax({
                                                        type: "POST",
                                                        url: url,
                                                        data: "",
                                                        success: function (data)
                                                        {
                                                            //var objData = jQuery.parseJSON(data);
                                                            jQuery("#collapse-occupiers7").show();
                                                            jQuery("#rowLoaderLinkPosition").hide();
                                                            jQuery("#keywordposition_rowdata").html(data);
                                                            document.getElementById("link_poition_status_box").value = 1;
                                                        }
                                                    });
                                                }
                                            }

                                            function getInternalLinksURL() {
                                                var internal_link_status_box = document.getElementById("internal_link_status_box").value;
                                                if (internal_link_status_box == '0') {
                                                    jQuery("#rowLoaderInternalLink").show();
                                                    jQuery("#collapse-occupiers11").hide();
                                                    var url = "<?php echo Mage::getBaseUrl(); ?>linkanalysis/index/getinternallinks?linkdetails_id=<?php echo $linkDetailsId; ?>";
                                                                jQuery.ajax({
                                                                    type: "POST",
                                                                    url: url,
                                                                    data: "",
                                                                    success: function (data)
                                                                    {
                                                                        var objData = jQuery.parseJSON(data);
                                                                        jQuery("#collapse-occupiers11").show();
                                                                        jQuery("#rowLoaderInternalLink").hide();
                                                                        if (parseInt(objData.status) == 1) {
                                                                            jQuery("#resultInternalLink").html(objData.html);
                                                                            jQuery("#count_internal_link").html("Total Internal Links No: " + objData.count);
                                                                        } else {
                                                                            jQuery("#count_internal_link").html("Total Internal Links No: 0");
                                                                            jQuery("#resultInternalLink").html("Sorry, No Internal Links Found!");
                                                                        }
                                                                        document.getElementById("internal_link_status_box").value = 1;
                                                                    }
                                                                });
                                                            }
                                                        }

                                                        function getExternalLinksURL() {
                                                            var external_link_status_box = document.getElementById("external_link_status_box").value;
                                                            if (external_link_status_box == '0') {
                                                                jQuery("#rowLoaderExternalLink").show();
                                                                jQuery("#collapse-occupiers3").hide();
                                                                var url = "<?php echo Mage::getBaseUrl(); ?>linkanalysis/index/getexternallinks?linkdetails_id=<?php echo $linkDetailsId; ?>";
                                                                            jQuery.ajax({
                                                                                type: "POST",
                                                                                url: url,
                                                                                data: "",
                                                                                success: function (data)
                                                                                {
                                                                                    var objData = jQuery.parseJSON(data);
                                                                                    jQuery("#collapse-occupiers3").show();
                                                                                    jQuery("#rowLoaderExternalLink").hide();
                                                                                    if (parseInt(objData.status) == 1) {
                                                                                        jQuery("#resultExternalLink").html(objData.html);
                                                                                        jQuery("#count_external_link").html("Total External Links No: " + objData.count);
                                                                                    } else {
                                                                                        jQuery("#count_external_link").html("Total External Links No: 0");
                                                                                        jQuery("#resultExternalLink").html("Sorry, No External Links Found!");
                                                                                    }
                                                                                    document.getElementById("external_link_status_box").value = 1;
                                                                                }
                                                                            });
                                                                        }
                                                                    }

                                                                    function deleteKeywords(keyword_ids) {
                                                                        var linkDetailsId = "<?php echo $linkDetailsId; ?>";
                                                                        var ids = [];
                                                                        jQuery.each($("input[name='" + keyword_ids + "']:checked"), function () {
                                                                            ids.push(jQuery(this).val());
                                                                        });
                                                                        var keywordIds = ids.join(",");
                                                                        var url = "<?php echo Mage::getBaseUrl(); ?>linkanalysis/index/deletekeyword?keywords_name=" + keywordIds + "&linkDetailsId=" + linkDetailsId;

                                                                        if (keywordIds.length > 0) {
                                                                            jQuery("#pre-loader").css("opacity", '0.8');
                                                                            jQuery("#pre-loader").show();
                                                                            jQuery.ajax({
                                                                                type: "POST",
                                                                                url: url,
                                                                                data: "",
                                                                                success: function (data)
                                                                                {
                                                                                    var objData = jQuery.parseJSON(data);
                                                                                    document.getElementById("keywordTableCustom").innerHTML = objData.content;
                                                                                    jQuery("#pre-loader").hide();
                                                                                }
                                                                            });
                                                                        } else {
                                                                            alert("Please select keywords");
                                                                        }
                                                                    }

                                                                    function addKeywords(linkDetailsId) {
                                                                        var keywords_name = jQuery("#keywords_name").val();
                                                                        var url = "<?php echo Mage::getBaseUrl(); ?>linkanalysis/index/addkeyword?keywords_name=" + keywords_name + "&linkDetailsId=" + linkDetailsId;
                                                                        if (keywords_name == '' || keywords_name == null) {
                                                                            document.getElementById("submitkeywordname").click();
                                                                        } else {
                                                                            jQuery("#pre-loader").css("opacity", '0.8');
                                                                            jQuery("#pre-loader").show();
                                                                            jQuery.ajax({
                                                                                type: "POST",
                                                                                url: url,
                                                                                data: "",
                                                                                success: function (data)
                                                                                {
                                                                                    var objData = jQuery.parseJSON(data);
                                                                                    document.getElementById("keywordTableCustom").innerHTML = objData.content;
                                                                                    jQuery("#pre-loader").hide();
                                                                                    var keyword_analysis_status_box = document.getElementById("keyword_analysis_status_box").value;
                                                                                    if (parseInt(keyword_analysis_status_box) >= 1) {
                                                                                        document.getElementById("keyword_analysis_status_box").value = 0;
                                                                                        document.getElementById("keyword_analysis_label").click();
                                                                                    }
                                                                                    var link_poition_status_box = document.getElementById("link_poition_status_box").value;
                                                                                    if (parseInt(link_poition_status_box) >= 1) {
                                                                                        document.getElementById("link_poition_status_box").value = 0;
                                                                                        document.getElementById("keywordPositionLabel").click();
                                                                                    }
                                                                                }
                                                                            });
                                                                        }
                                                                    }

                                                                    function showMetaTitle(metaTitle) {
                                                                        jQuery("#addKeyWordSec").hide();
                                                                        jQuery("#metatitlesection").show();
                                                                        jQuery("#section_title").html("<p style='padding-left:10px;' class='card-text text-dark'><b>META TITLE</b></p>");
                                                                        if (metaTitle.length > 1) {
                                                                            jQuery("#metatitlesection").html(metaTitle);
                                                                        } else {
                                                                            jQuery("#metatitlesection").html('<label style="font-size:13px;padding-bottom: 10px; padding-top: 8px;" class="badge badge-danger">Meta Title Not Found!</label>');
                                                                        }
                                                                    }

                                                                    function showMetaDescription(metaDescription) {
                                                                        jQuery("#addKeyWordSec").hide();
                                                                        jQuery("#metatitlesection").show();
                                                                        jQuery("#section_title").html("<p style='padding-left:10px;' class='card-text text-dark'><b>META DESCRIPTION</b></p>");
                                                                        if (metaDescription.length > 1) {
                                                                            jQuery("#metatitlesection").html(metaDescription);
                                                                        } else {
                                                                            jQuery("#metatitlesection").html('<label style="font-size:13px;padding-bottom: 10px;padding-top: 8px;" class="badge badge-danger">Meta Description Not Found!</label>');
                                                                        }
                                                                    }

                                                                    function showKeyWords(metaKeywords) {
                                                                        jQuery("#addKeyWordSec").hide();
                                                                        jQuery("#metatitlesection").show();
                                                                        jQuery("#section_title").html("<p style='padding-left:10px;' class='card-text text-dark'><b>META KEYWORDS</b></p>");
                                                                        if (metaKeywords.length > 1) {
                                                                            jQuery("#metatitlesection").html(metaKeywords);
                                                                        } else {
                                                                            jQuery("#metatitlesection").html('<label style="font-size:13px;padding-bottom: 10px; padding-top: 8px;" class="badge badge-danger">Meta Keywords Not Found!</label>');
                                                                        }
                                                                    }

                                                                    function addKeyWord(referenceLinkId, metaKeywords) {
                                                                        jQuery("#section_title").html("<p style='padding-left:20px;' class='card-text text-dark'><b>ADD KEYWORDS</b></p>");
                                                                        jQuery("#metatitlesection").hide();
                                                                        jQuery("#addKeyWordSec").show();
                                                                    }

                                                                    function showIndexCount() {
                                                                        var index_status_box = document.getElementById("index_status_box").value;
                                                                        if (index_status_box == '0') {
                                                                            jQuery("#rowLoaderIndexCount").show();
                                                                            jQuery("#collapse-occupiers2").hide();
                                                                            var url = "<?php echo Mage::getBaseUrl(); ?>linkanalysis/index/getindexcount?linkdetails_id=<?php echo $linkDetailsId; ?>";
                                                                                        jQuery.ajax({
                                                                                            type: "POST",
                                                                                            url: url,
                                                                                            data: "",
                                                                                            success: function (data)
                                                                                            {
                                                                                                var objData = jQuery.parseJSON(data);
                                                                                                if (parseInt(objData.google_count) > 0) {
                                                                                                    jQuery("#google_index_count_icon").html('<i style="color:green;" class="fa fa-check-square-o highlight-icon" aria-hidden="true"></i>');
                                                                                                    document.getElementById("index_google_label").setAttribute("class", "badge badge-success");
                                                                                                    jQuery("#index_google_label").html("INDEXED");
                                                                                                    jQuery("#google_count_num").html(" " + objData.google_count);
                                                                                                } else {
                                                                                                    jQuery("#google_index_count_icon").html('<i style="color:#dc3545;" class="fa fa-times-circle-o highlight-icon" aria-hidden="true"></i>');
                                                                                                    document.getElementById("index_google_label").setAttribute("class", "badge badge-danger");
                                                                                                    jQuery("#index_google_label").html("NOT INDEXED");
                                                                                                    jQuery("#google_count_num").html("0");
                                                                                                }

                                                                                                if (parseInt(objData.bing_count) > 0) {
                                                                                                    jQuery("#bing_index_count_icon").html('<i style="color:green;" class="fa fa-check-square-o highlight-icon" aria-hidden="true"></i>');
                                                                                                    document.getElementById("index_bing_label").setAttribute("class", "badge badge-success");
                                                                                                    jQuery("#index_bing_label").html("INDEXED");
                                                                                                    jQuery("#bing_count_num").html(" " + objData.bing_count);
                                                                                                } else {
                                                                                                    jQuery("#bing_index_count_icon").html('<i style="color:#dc3545;" class="fa fa-times-circle-o highlight-icon" aria-hidden="true"></i>');
                                                                                                    document.getElementById("index_bing_label").setAttribute("class", "badge badge-danger");
                                                                                                    jQuery("#index_bing_label").html("NOT INDEXED");
                                                                                                    jQuery("#bing_count_num").html("0");
                                                                                                }

                                                                                                if (parseInt(objData.yahoo_count) > 0) {
                                                                                                    jQuery("#yahoo_index_count_icon").html('<i style="color:green;" class="fa fa-check-square-o highlight-icon" aria-hidden="true"></i>');
                                                                                                    document.getElementById("index_yahoo_label").setAttribute("class", "badge badge-success");
                                                                                                    jQuery("#index_yahoo_label").html("INDEXED");
                                                                                                    jQuery("#yahoo_count_num").html(" " + objData.yahoo_count);
                                                                                                } else {
                                                                                                    jQuery("#yahoo_index_count_icon").html('<i style="color:#dc3545;" class="fa fa-times-circle-o highlight-icon" aria-hidden="true"></i>');
                                                                                                    document.getElementById("index_yahoo_label").setAttribute("class", "badge badge-danger");
                                                                                                    jQuery("#index_yahoo_label").html("NOT INDEXED");
                                                                                                    jQuery("#yahoo_count_num").html("0");
                                                                                                }

                                                                                                if (parseInt(objData.yandex_count) > 0) {
                                                                                                    jQuery("#yandex_index_count_icon").html('<i style="color:green;" class="fa fa-check-square-o highlight-icon" aria-hidden="true"></i>');
                                                                                                    document.getElementById("index_yandex_label").setAttribute("class", "badge badge-success");
                                                                                                    jQuery("#index_yandex_label").html("INDEXED");
                                                                                                    jQuery("#yandex_count_num").html(" " + objData.yandex_count);
                                                                                                } else {
                                                                                                    jQuery("#yandex_index_count_icon").html('<i style="color:#dc3545;" class="fa fa-times-circle-o highlight-icon" aria-hidden="true"></i>');
                                                                                                    document.getElementById("index_yandex_label").setAttribute("class", "badge badge-danger");
                                                                                                    jQuery("#index_yandex_label").html("NOT INDEXED");
                                                                                                    jQuery("#yandex_count_num").html("0");
                                                                                                }



                                                                                                jQuery("#rowLoaderIndexCount").hide();
                                                                                                jQuery("#collapse-occupiers2").show();

                                                                                                document.getElementById("index_status_box").value = 1;
                                                                                            }
                                                                                        });
                                                                                    }
                                                                                }

                                                                                function showSocialShare() {
                                                                                    var social_share_status_box = document.getElementById("social_share_status_box").value;
                                                                                    if (social_share_status_box == '0') {
                                                                                        jQuery("#collapse-occupiers1").hide();
                                                                                        jQuery("#rowLoaderSocialShare").show();
                                                                                        var url = "<?php echo Mage::getBaseUrl(); ?>linkanalysis/index/getsocialsharecount?linkdetails_id=<?php echo $linkDetailsId; ?>";
                                                                                                    jQuery.ajax({
                                                                                                        type: "POST",
                                                                                                        url: url,
                                                                                                        data: "",
                                                                                                        success: function (data)
                                                                                                        {
                                                                                                            var objData = jQuery.parseJSON(data);
                                                                                                            document.getElementById("facebook_count").innerHTML = objData.fb_count;
                                                                                                            document.getElementById("twitter_count").innerHTML = "0";
                                                                                                            document.getElementById("youtube_count").innerHTML = "0";
                                                                                                            document.getElementById("instagram_count").innerHTML = "0";
                                                                                                            document.getElementById("linkedin_count").innerHTML = "0";
                                                                                                            document.getElementById("pinterest_count").innerHTML = objData.pintrest_count;
                                                                                                            document.getElementById("google_count").innerHTML = "0";
                                                                                                            document.getElementById("timblr_count").innerHTML = "0";

                                                                                                            jQuery("#rowLoaderSocialShare").hide();
                                                                                                            jQuery("#collapse-occupiers1").show();

                                                                                                            document.getElementById("social_share_status_box").value = 1;
                                                                                                        }
                                                                                                    });
                                                                                                }
                                                                                            }

                                                                                            function setCuntryLocation(e) {
                                                                                                var countryCode = e.value;
                                                                                                var url = "<?php echo Mage::getBaseUrl(); ?>linkanalysis/index/setcountry?linkdetails_id=<?php echo $linkDetailsId; ?>&countryCode=" + countryCode;
                                                                                                //jQuery("pre-loader").show();
                                                                                                jQuery.ajax({
                                                                                                    type: "POST",
                                                                                                    url: url,
                                                                                                    data: "",
                                                                                                    success: function (data)
                                                                                                    {
                                                                                                        //jQuery("pre-loader").hide();
                                                                                                        /*var objData = jQuery.parseJSON(data);
                                                                                                         if(parseInt(objData.status) == 1){
                                                                                                         document.location.href = "<?php //echo Mage::getBaseUrl();         ?>analysis-result?linkDetailsId=<?php echo $linkDetailsId; ?>&cn="+countryCode;
                                                                                                         }else{
                                                                                                         alert("Sorry! Something wrong");
                                                                                                         }*/
                                                                                                        document.location.href = "<?php echo Mage::getBaseUrl(); ?>analysis-result?linkDetailsId=<?php echo $linkDetailsId; ?>&cn=" + countryCode;
                                                                                                    }
                                                                                                });
                                                                                            }
</script>